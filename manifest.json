{
  "name": "Web Startup Toolbar",
  "version": "1.2",
  "description": "Pulls data from CrunchBase, LinkedIn, Quantcast, Compete, Alexa, and Google/Bing/Yahoo on any website being viewed",
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Web Startup Toolbar",
    "popup": "popup.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "background_page": "background.html",
  "options_page": "options.html"
}